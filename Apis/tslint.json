{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-console": [false, "log", "error"],
    "indent": [false, "tabs", 4],
    "quotemark": [true, "double", "jsx-double"],
    "semi": ["error", "always"],
    "max-line-length": [true, 300],
    "eofline": true,
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}